!methodRemoval: Integer #convertUsing:unit:five:ten: stamp: 'crivera 5/10/2021 21:39:46'!
convertUsing: stream unit: aUnit five: aFive ten: aTen


		(self between: 1 and: 3) ifTrue: [self timesRepeat: [ stream nextPut: aUnit ]].
		self = 4 ifTrue: [stream nextPut: aUnit .
						stream nextPut: aFive ].
		(self between: 5 and: 8) ifTrue: [stream nextPut: aFive .
			(self - 5) timesRepeat: [ stream nextPut: aUnit ] ].
		self = 9 ifTrue: [stream nextPut: aUnit.
					stream nextPut: aTen ].!

!methodRemoval: Integer #m1: stamp: 'crivera 5/10/2021 21:39:46'!
m1: stream

	(self >= 50) ifTrue: [
		stream nextPutAll: 'L'.
		^(self - 50) m1: stream].
	(self >= 40) ifTrue: [
		stream nextPutAll: 'XL'.
		^(self - 40) m1: stream].
	(self >= 10) ifTrue: [
		stream nextPutAll: 'X'.
		^(self - 10) m1: stream].
	^ self convertDigitUsing: stream.
	
	
	
