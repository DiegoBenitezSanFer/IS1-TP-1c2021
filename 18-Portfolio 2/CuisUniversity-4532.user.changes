asRomanString

	^String streamContents: [ :stream |
		
		(self >= 40) ifTrue: [stream nextPutAll: 'XL'.
			(self - 40) convertDigitUsing: stream]
		ifFalse:[
			
			(self // 10) timesRepeat: [
				stream nextPut: $X].
			(self \\ 10) convertDigitUsing: stream
		]
	]! !

